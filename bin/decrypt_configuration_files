#!/usr/bin/env bash

# Create bundle-latest

set -e

PASSWORD=$1

if [ -z "$PASSWORD" ]
then
  echo "No password parameter given, trying to read from $HOME/.mediheroes/secret.key"
  openssl aes-256-cbc -d -a -in .env.enc -out .env -pass file:$HOME/.mediheroes/secret.key
else
  openssl aes-256-cbc -d -a -in .env.enc -out .env -pass pass:$PASSWORD
fi


